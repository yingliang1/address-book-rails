<body>
<h1>Login</h1>
<table class="center">
<thead>
   <%= form_tag '/login' do %>
   <th><%= label_tag :username%></th>
   <th><%= text_field_tag :username %></th>
   <tr>
   <td><%= label_tag :password%></td>
   <td><%= password_field_tag :password%></td>
   </tr>
   <tr>
   <td></td>
   <td><%= submit_tag "Login"%></td>
   </tr>
<%end%>
</table>
<%= button_to "Exit", '/welcome', method: :get%>

</body>

