<%= form_for @contact do |f| %>
     <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= f.label :address %>
      <%= f.text_field :address %>
    </div>
    <div>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>
    <div>
      <%= f.label :phone %>
      <%= f.number_field :phone %>
    </div>
    <div>
      <%= f.submit %>
    </div>
    <% end %>
